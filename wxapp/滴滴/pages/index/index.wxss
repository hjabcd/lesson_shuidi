page{
  width: 100%;
  height: 100vh;
  background-color: #f4f6f8;
}
.container{
  /* 任何设备在小程序中都是750rpx */
  /* rpx 对应的是750px */
  position: fixed;
  width: 686rpx;
  height: 438rpx;
  background-color: green;
  margin-top: 32rpx;
  margin-left: 32rpx;
  top: 140rpx;
  box-shadow: 1px 1px 6px #f4f6f8;
}
.section{
  height: 108rpx;
  width: 100%;
  background-color: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
}
.nav-me,.nav-all{
  width: 30rpx;
  height: 30rpx;
  background-color: #fff;
  transform: scale(0.18);
  z-index: 999;
}
.nav-me{
  left: 0;
  margin-top: 10rpx;
}
.nav-all{
  right: 0rpx;
  position: absolute;
  top: 0;
  margin-right: 80rpx;
}

.nav{
  height: 108rpx;
  width: 550rpx;
  position: fixed;
  box-sizing: border-box;
  overflow: hidden;  /* 超出隐藏 */
  line-height: 108rpx;
  background: #ffffff;
  font-size: 16px;
  white-space: nowrap;
  top:0;
  left: 100rpx;
  right: 100rpx;
  z-index: 99;
  margin-bottom: 32px;
}
.nav-item{
  display: inline-block;
  margin-left: 20rpx;
  margin-right: 20rpx;
  text-align: center;
}
.nav-item.active{
  color:#fc975c;
}